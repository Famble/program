<Pane maxHeight="-Infinity" onKeyPressed="#keyListener" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800" prefWidth="1200" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Canvas fx:id="canvas" onMouseClicked="#mouseClicked" height="500" width="800" onMouseDragged="#mouseDragged" onKeyPressed="#keyListener"/>
      <Box layoutX="800" layoutY="800" />
