      <Button fx:id="startButton" blendMode="EXCLUSION" layoutX="29.0" layoutY="430" mnemonicParsing="false" onAction="#handleStartClick" prefHeight="48.0" prefWidth="95.0" text="Start">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button blendMode="EXCLUSION" layoutX="310" layoutY="430" mnemonicParsing="false" onAction="#handlePauseClick" prefHeight="48.0" prefWidth="95.0" text="Pause">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button blendMode="EXCLUSION" layoutX="170" layoutY="430" mnemonicParsing="false" onAction="#handleResetClick" prefHeight="48.0" prefWidth="95.0" text="Reset">
