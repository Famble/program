         <effect>
            <Lighting diffuseConstant="1.4" specularConstant="2.0" specularExponent="38.29" surfaceScale="0.93">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant azimuth="77.14" />
               </light>
            </Lighting>
         </effect></ColorPicker>
      <Label layoutX="616.0" layoutY="460" text="Color">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label layoutX="440.0" layoutY="460" text="Game speed">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
   </children>
</Pane>
